}

										/* Gestion des GroupBox */
QTabWidget QGroupBox
{
	margin: 1px;
	margin-left: 4px;
	padding-top: 9px;
	border: 1px solid black;
	border-radius: 5px;
}

QTabWidget QGroupBox::title
{
	padding-top: -5px;
	padding-left: 0px;
	margin-left: 0;
	font-weight: bold;	
}


										/* Gestion des LineEdit */
QTabWidget QLineEdit
{
	border: 1px solid black;
	Background-color: #fff;
	color: rgb(58, 117, 175);
	border-radius: 5px;
	padding-right: 7px;
	padding-left: 7px;
}

QTabWidget QLineEdit:hover{
	border: 2px solid rgb(150, 170, 250);
	color: rgb(84, 0, 0);
}

