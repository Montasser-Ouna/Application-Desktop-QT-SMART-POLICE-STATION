QTabWidget QLineEdit:hover{
	border: 2px solid rgb(150, 170, 250);
	color: rgb(84, 0, 0);
}

QTabWidget QLineEdit:focus{
	border: 2px solid rgb(100, 170, 255);
	color: rgb(84, 0, 0);
}

										/* Gestion des ScrollArea*/ 
QTabWidget QScrollArea
{
	background-color: #fff;
}

QTabWidget QScrollArea:vertical
{
	background-color: black;
}

										/* ---- Gestion des PushButton ---- */
QTabWidget QPusButton
{
	border-radius: 5px;
}

										/* ---- Gestion des TableView ---- */

QTableView
{
	background-color: #fff;	
	alternate-background-color: #c9dce6;
	Selection-background-color: #bccfc3; /* couleur verte */
	Border : 2px solid #000000;
	border-radius: 5px;
	font-size: 10pt;
}

/* button at the upper left corner */
QTableView QTableCornerButton::section
{
	border-top: 1px solid #617c97;
	Background-color: #f0e9d3;
}

										/* Gestion des Header de tableView */
/* table header style */
QHeaderView::section {
    background-color: #617c97;
    font-size: 10pt;
    border-style: none;
    border-bottom: 1px solid #fffff8;
    border-right: 1px solid #fffff8;
	height: 20px; /* header height */
}

QHeaderView::section:horizontal
{
    border-top: 1px solid #617c97;
}

QHeaderView::section:vertical
{
